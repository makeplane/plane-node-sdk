{
  "name": "@plane/node-sdk",
  "version": "1.0.0",
  "description": "Node.js SDK for Plane API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:e2e": "jest --selectProjects e2e",
    "test:comprehensive": "npx ts-node tests/run-comprehensive-test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "plane",
    "sdk",
    "api",
    "typescript",
    "node"
  ],
  "author": "Plane Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "ts-jest": "^29.4.4"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
